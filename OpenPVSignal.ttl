@prefix : <http://purl.org/OpenPVSignal/OpenPVSignal.owl> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix mp: <http://purl.org/mp/> .
@prefix oa: <http://www.w3.org/ns/oa#> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix opv: <http://inab.certh.gr/OpenPVSignal> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <http://purl.obolibrary.org/obo/uberon/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix pato: <http://purl.obolibrary.org/obo/pato#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix core1: <http://purl.obolibrary.org/obo/core#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix uberon: <http://purl.obolibrary.org/obo/uberon#> .
@prefix subsets: <http://purl.obolibrary.org/obo/ro/subsets#> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@prefix OpenPVSignal: <http://purl.org/OpenPVSignal/OpenPVSignal.owl#> .
@prefix openpvsignal: <http://purl.org/OpenPVSignal/OpenPVSignal.owl> .
@base <http://purl.org/OpenPVSignal/OpenPVSignal.owl> .

<http://purl.org/OpenPVSignal/OpenPVSignal.owl> rdf:type owl:Ontology ;
                                                 owl:imports obo:oae.owl ,
                                                             <http://purl.org/mp> ,
                                                             <http://www.w3.org/2006/time#2016> ,
                                                             oa: ;
                                                 rdfs:comment "OpenPVSignal ontology describes a model able to present pharmacovigilance signal information, originally contained in free-text reports."^^rdfs:Literal ;
                                                 rdfs:label "OpenPVSignal"^^rdfs:Literal ;
                                                 owl:versionInfo "draft-v0.9-20200521"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.obolibrary.org/obo/IAO_0000119
obo:IAO_0000119 rdf:type owl:AnnotationProperty .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_ATC_code
OpenPVSignal:has_ATC_code rdf:type owl:AnnotationProperty ;
                          rdfs:comment "Identifies the ATC code of the respective drug." ;
                          rdfs:label "has ATC code" ;
                          rdfs:subPropertyOf oboInOwl:hasDbXref ;
                          rdfs:range xsd:string .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_ICD_code
OpenPVSignal:has_ICD_code rdf:type owl:AnnotationProperty ;
                          rdfs:comment "Identifies the ICD code of the respective condition." ;
                          rdfs:label "has ICD code" ;
                          rdfs:subPropertyOf oboInOwl:hasDbXref ;
                          rdfs:range xsd:string .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_MeSH_tree_number
OpenPVSignal:has_MeSH_tree_number rdf:type owl:AnnotationProperty ;
                                  rdfs:comment "Identifies the MeSH tree code of the respective entity." ;
                                  rdfs:label "has MeSH tree number" ;
                                  rdfs:subPropertyOf oboInOwl:hasDbXref ;
                                  rdfs:range xsd:string .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_MedDRA_code
OpenPVSignal:has_MedDRA_code rdf:type owl:AnnotationProperty ;
                             rdfs:comment "Identifies the MedDRA code of the respective adverse effect." ;
                             rdfs:label "has MedDRA code" ;
                             rdfs:subPropertyOf oboInOwl:hasDbXref ;
                             rdfs:range xsd:int ;
                             rdfs:domain OpenPVSignal:Adverse_Effect .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_MedDRA_prefered_term
OpenPVSignal:has_MedDRA_prefered_term rdf:type owl:AnnotationProperty ;
                                      rdfs:comment "Identifies the MedDRA preferred term regarding the respective adverse effect." ;
                                      rdfs:label "has MedDRA prefered term" ;
                                      rdfs:subPropertyOf oboInOwl:hasDbXref ;
                                      rdfs:range xsd:string ;
                                      rdfs:domain OpenPVSignal:Adverse_Effect .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_RxNorm_code
OpenPVSignal:has_RxNorm_code rdf:type owl:AnnotationProperty ;
                             rdfs:comment "Identifies the RxNorm code of the respective drug." ;
                             rdfs:label "has RxNorm code" ;
                             rdfs:subPropertyOf oboInOwl:hasDbXref ;
                             rdfs:range xsd:string ;
                             rdfs:domain OpenPVSignal:Drug .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_SNOMED-CT_code
OpenPVSignal:has_SNOMED-CT_code rdf:type owl:AnnotationProperty ;
                                rdfs:comment "Indentifies the SNOMED-CT code of the respective entity" ;
                                rdfs:label "has SNOMED-CT code" ;
                                rdfs:subPropertyOf oboInOwl:hasDbXref ;
                                rdfs:range xsd:string .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_drugbank_code
OpenPVSignal:has_drugbank_code rdf:type owl:AnnotationProperty ;
                               rdfs:comment "Identifies the drugbank code of the respective drug." ;
                               rdfs:label "has drugbank code" ;
                               rdfs:subPropertyOf oboInOwl:hasDbXref ;
                               rdfs:range xsd:string ;
                               rdfs:domain OpenPVSignal:Drug .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.geneontology.org/formats/oboInOwl#hasBroadSynonym
oboInOwl:hasBroadSynonym rdf:type owl:AnnotationProperty .


###  http://www.geneontology.org/formats/oboInOwl#hasDbXref
oboInOwl:hasDbXref rdf:type owl:AnnotationProperty .


###  http://www.geneontology.org/formats/oboInOwl#hasExactSynonym
oboInOwl:hasExactSynonym rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/oa#hasTarget
oa:hasTarget rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYearMonth
xsd:gYearMonth rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#belongs_to_reports_group
OpenPVSignal:belongs_to_reports_group rdf:type owl:ObjectProperty ;
                                      rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                                      rdfs:range OpenPVSignal:Reports_group ;
                                      rdfs:comment "Relates a spontaneous report with a report group." ;
                                      rdfs:label "belongs to reports group" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#concerns_indication_for_use
OpenPVSignal:concerns_indication_for_use rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                         rdfs:domain [ rdf:type owl:Class ;
                                                       owl:unionOf ( OpenPVSignal:Drug
                                                                     OpenPVSignal:DrugClass
                                                                     OpenPVSignal:Drug_Usage
                                                                     OpenPVSignal:Reports_group
                                                                   )
                                                     ] ;
                                         rdfs:range OpenPVSignal:Indication ;
                                         rdfs:comment "Identifies the condition which can be considered the reason for the respective drug administration." ;
                                         rdfs:label "concerns indication for use"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#hasClass
OpenPVSignal:hasClass rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf OpenPVSignal:refers_to_class ;
                      rdfs:domain OpenPVSignal:Drug ;
                      rdfs:range OpenPVSignal:DrugClass ;
                      rdfs:comment "Relates a drug with the drug class that it belongs to." ;
                      rdfs:label "belongs to class" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_free_text_reporting_element
OpenPVSignal:has_free_text_reporting_element rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf mp:hasElement ;
                                             rdf:type owl:InverseFunctionalProperty ;
                                             rdfs:domain OpenPVSignal:Pharmacovigilance_Signal_Report ;
                                             rdfs:range OpenPVSignal:Free_text_reporting_element ;
                                             rdfs:comment "Relates a free-text reporting element with the pharmacovigilance report that it belongs to." ;
                                             rdfs:label "has free text reporting element"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_mechanism
OpenPVSignal:has_mechanism rdf:type owl:ObjectProperty ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( OpenPVSignal:Adverse_Effect
                                                       OpenPVSignal:Drug
                                                       OpenPVSignal:DrugClass
                                                     )
                                       ] ;
                           rdfs:range OpenPVSignal:Mechanism ;
                           rdfs:comment "Depicts the relation between a drug or a drug class and a mechanism of action." ;
                           rdfs:label "has mechanism" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#has_potential_confounding_factor
OpenPVSignal:has_potential_confounding_factor rdf:type owl:ObjectProperty ;
                                              rdfs:comment "Relates a Signal or report/reports group to a potential confounding factor." ;
                                              rdfs:label "has potential confounding factor" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_part_of
OpenPVSignal:is_part_of rdf:type owl:ObjectProperty ,
                                 owl:TransitiveProperty ;
                        rdfs:domain OpenPVSignal:Free_text_reporting_element ;
                        rdfs:range OpenPVSignal:Free_text_reporting_element ;
                        rdfs:comment "Depicts that a free-text snippet could be part of a larger free-text section." ;
                        rdfs:label "is part of" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_related_with_drug_exposure
OpenPVSignal:is_related_with_drug_exposure rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain OpenPVSignal:Drug_Usage ;
                                           rdfs:range OpenPVSignal:Drug_Exposure_Time ;
                                           rdfs:comment "Relates the information regarding a specific drug usage with the respective time exposure." ;
                                           rdfs:label "is related with drug exposure"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_related_with_drug_intake
OpenPVSignal:is_related_with_drug_intake rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                         rdfs:domain OpenPVSignal:Drug_Usage ,
                                                     OpenPVSignal:Reports_group ;
                                         rdfs:range OpenPVSignal:Drug_Intake_Form ;
                                         rdfs:comment "Relates the usage of a drug with a specific intake form." ;
                                         rdfs:label "is related with drug intake form"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_subgroup_of
OpenPVSignal:is_subgroup_of rdf:type owl:ObjectProperty ;
                            rdfs:domain OpenPVSignal:Reports_group ;
                            rdfs:range OpenPVSignal:Reports_group ;
                            rdfs:comment "Enables the identification of a spontaneous report group as part of another spontaneous report group." ;
                            rdfs:label "is subgroup of" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_supported_by_cohort_profile
OpenPVSignal:is_supported_by_cohort_profile rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf mp:supportedByData ;
                                            rdfs:domain OpenPVSignal:Signal ;
                                            rdfs:range OpenPVSignal:Cohort_Profile ;
                                            rdfs:comment "Relates a pharmacovigilance signal with a cohort profile." ;
                                            rdfs:label "is supported by cohort profile" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_supported_by_individual_case_report
OpenPVSignal:is_supported_by_individual_case_report rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf mp:supportedByData ;
                                                    rdfs:domain OpenPVSignal:Signal ;
                                                    rdfs:range OpenPVSignal:Individual_Case_Safety_Report ;
                                                    rdfs:comment "Relates a pharmacovigilance signal with the corresponding individual case reports." ;
                                                    rdfs:label "is supported by individual case report"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#is_supported_by_statistical_entity
OpenPVSignal:is_supported_by_statistical_entity rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf mp:supportedByData ;
                                                rdf:type owl:InverseFunctionalProperty ;
                                                rdfs:domain [ rdf:type owl:Class ;
                                                              owl:unionOf ( OpenPVSignal:Signal
                                                                            OpenPVSignal:Statistical_Entity
                                                                          )
                                                            ] ;
                                                rdfs:range OpenPVSignal:Statistical_Entity ;
                                                rdfs:comment "Relates a pharmacovigilance signal with the supporting statistical data." ;
                                                rdfs:label "is supported by statistical entity"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_adverse_effect
OpenPVSignal:refers_to_adverse_effect rdf:type owl:ObjectProperty ,
                                               owl:TransitiveProperty ;
                                      rdfs:domain [ rdf:type owl:Class ;
                                                    owl:unionOf ( OpenPVSignal:Adverse_Effect
                                                                  OpenPVSignal:Disproportionality_Analysis_Measure
                                                                  OpenPVSignal:Drug
                                                                  OpenPVSignal:Individual_Case_Safety_Report
                                                                  OpenPVSignal:Medication_Errors
                                                                  OpenPVSignal:Signal
                                                                  OpenPVSignal:Warning_Information
                                                                )
                                                  ] ;
                                      rdfs:range OpenPVSignal:Adverse_Effect ;
                                      rdfs:comment "Depicts a reference to the respective adverse effect." ;
                                      rdfs:label "refers to adverse effect"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_author
OpenPVSignal:refers_to_author rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf mp:attributionAsAuthor ,
                                                 mp:hasAttribution ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( OpenPVSignal:Pharmacovigilance_Signal_Report
                                                          OpenPVSignal:Response_to_Pharmacovigilance_Signal_Report
                                                        )
                                          ] ;
                              rdfs:range OpenPVSignal:Author ;
                              rdfs:comment "Identifies the author that the respective manuscript refers to." ;
                              rdfs:label "refers to author"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_class
OpenPVSignal:refers_to_class rdf:type owl:ObjectProperty ;
                             rdfs:range OpenPVSignal:DrugClass ;
                             rdfs:comment "Depicts a reference with the respective drug class." ;
                             rdfs:label "refers to class" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_cohort_profile
OpenPVSignal:refers_to_cohort_profile rdf:type owl:ObjectProperty ;
                                      rdfs:domain [ rdf:type owl:Class ;
                                                    owl:unionOf ( OpenPVSignal:Cohort_Profile
                                                                  OpenPVSignal:Disproportionality_Analysis_Measure
                                                                  OpenPVSignal:Dosage
                                                                  OpenPVSignal:DrugClass
                                                                  OpenPVSignal:Drug_Exposure_Time
                                                                  OpenPVSignal:Drug_Intake_Form
                                                                  OpenPVSignal:Drug_Usage
                                                                  OpenPVSignal:Individual_Case_Safety_Report
                                                                  OpenPVSignal:Number_of_prescriptions_on_population
                                                                  OpenPVSignal:Signal
                                                                  OpenPVSignal:Warning_Information
                                                                )
                                                  ] ;
                                      rdfs:range [ rdf:type owl:Class ;
                                                   owl:unionOf ( OpenPVSignal:Incidence
                                                                 OpenPVSignal:Prevalence
                                                               )
                                                 ] ;
                                      rdfs:comment "Depicts a reference for Incidence or Prevalence to a cohort profile." ;
                                      rdfs:label "refers to cohort profile" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_concomitant_drug
OpenPVSignal:refers_to_concomitant_drug rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf OpenPVSignal:refers_to_drug ;
                                        rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                                        rdfs:comment "Explicitly states that the respective drug is reported as a concomittant drug in the specific spontaneous report." ;
                                        rdfs:label "refers to concomitant drug"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_confidence_interval
OpenPVSignal:refers_to_confidence_interval rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf OpenPVSignal:is_supported_by_statistical_entity ;
                                           rdfs:range OpenPVSignal:Confidence_interval_0.95 ;
                                           rdfs:comment "Depicts a relation with a specific confidence interval." ;
                                           rdfs:label "refers to confidence interval" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_country
OpenPVSignal:refers_to_country rdf:type owl:ObjectProperty ;
                               rdfs:domain mp:Data ;
                               rdfs:range <http://sweet.jpl.nasa.gov/2.3/humanJurisdiction.owl#Country> .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_dosage
OpenPVSignal:refers_to_dosage rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain OpenPVSignal:Drug_Usage ;
                              rdfs:range OpenPVSignal:Dosage ;
                              rdfs:comment "Relates a drug usage with a specifc dosage." ;
                              rdfs:label "refers to dosage" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_drug
OpenPVSignal:refers_to_drug rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( OpenPVSignal:Disproportionality_Analysis_Measure
                                                        OpenPVSignal:Dosage
                                                        OpenPVSignal:DrugClass
                                                        OpenPVSignal:Drug_Exposure_Time
                                                        OpenPVSignal:Drug_Intake_Form
                                                        OpenPVSignal:Drug_Usage
                                                        OpenPVSignal:Individual_Case_Safety_Report
                                                        OpenPVSignal:Number_of_prescriptions_on_population
                                                        OpenPVSignal:Signal
                                                        OpenPVSignal:Warning_Information
                                                      )
                                        ] ;
                            rdfs:range OpenPVSignal:Drug ;
                            rdfs:comment "Depicts a relation with the respective drug." ;
                            rdfs:label "refers to drug"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_drug_drug_interaction
OpenPVSignal:refers_to_drug_drug_interaction rdf:type owl:ObjectProperty ;
                                             rdfs:domain [ rdf:type owl:Class ;
                                                           owl:unionOf ( OpenPVSignal:Adverse_Effect
                                                                         OpenPVSignal:Disproportionality_Analysis_Measure
                                                                         OpenPVSignal:Drug
                                                                         OpenPVSignal:Individual_Case_Safety_Report
                                                                         OpenPVSignal:Medication_Errors
                                                                         OpenPVSignal:Signal
                                                                         OpenPVSignal:Warning_Information
                                                                       )
                                                         ] ;
                                             rdfs:range OpenPVSignal:Drug-Drug_interaction ;
                                             rdfs:comment "Depicts a reference to group of interacting drugs." ;
                                             rdfs:label "refers to drug-drug interraction" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_information_component
OpenPVSignal:refers_to_information_component rdf:type owl:ObjectProperty ;
                                             rdfs:domain <http://purl.org/OpenPVSignal/OpenPVSignal.owl#Information_Component_(IC0005)> ,
                                                         <http://purl.org/OpenPVSignal/OpenPVSignal.owl#Information_Component_005_(IC005)> ,
                                                         <http://purl.org/OpenPVSignal/OpenPVSignal.owl#Information_Component__025_(IC025)> ;
                                             rdfs:range <http://purl.org/OpenPVSignal/OpenPVSignal.owl#Information_Component_(IC)> ;
                                             rdfs:comment "Associates a lower point of am information component credibility interval to the information component it derives from." ;
                                             rdfs:label "refers to information component" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_interacting_drug
OpenPVSignal:refers_to_interacting_drug rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf OpenPVSignal:refers_to_drug ;
                                        rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ,
                                                    OpenPVSignal:Signal ;
                                        rdfs:comment "Explicitly states that the respective drug is reported as an interacting drug in the specific spontaneous report." ;
                                        rdfs:label "refers to interacting drug"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_interval_between_administrations
OpenPVSignal:refers_to_interval_between_administrations rdf:type owl:ObjectProperty ;
                                                        rdfs:domain OpenPVSignal:Dosage ;
                                                        rdfs:range <http://www.w3.org/2006/time#DurationDescription> ;
                                                        rdfs:comment "Identifies the time passed between two consecutive drug administrations." ;
                                                        rdfs:label "refers to interval between administrations" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_known_adverse_effect
OpenPVSignal:refers_to_known_adverse_effect rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf OpenPVSignal:refers_to_adverse_effect ;
                                            rdfs:comment "Depicts a reference to the respective known adverse effect." ;
                                            rdfs:label "refers to known adverse effect" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_medication_error
OpenPVSignal:refers_to_medication_error rdf:type owl:ObjectProperty ;
                                        rdfs:domain [ rdf:type owl:Class ;
                                                      owl:unionOf ( OpenPVSignal:Adverse_Effect
                                                                    OpenPVSignal:Disproportionality_Analysis_Measure
                                                                    OpenPVSignal:Drug
                                                                    OpenPVSignal:Individual_Case_Safety_Report
                                                                    OpenPVSignal:Medication_Errors
                                                                    OpenPVSignal:Signal
                                                                    OpenPVSignal:Warning_Information
                                                                  )
                                                    ] ;
                                        rdfs:range OpenPVSignal:Medication_Errors ;
                                        rdfs:comment "Depicts a reference to the respective medication error." ;
                                        rdfs:label "refers to medication error" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_past_diagnosis
OpenPVSignal:refers_to_past_diagnosis rdf:type owl:ObjectProperty ;
                                      rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                                      rdfs:range OpenPVSignal:Past_Diagnosis ;
                                      rdfs:comment "Assosiates an Individual Case Safety Report with a past diagnosis." ;
                                      rdfs:label "refers to past diagnosis" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_patient
OpenPVSignal:refers_to_patient rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf mp:attributedTo ;
                               rdf:type owl:FunctionalProperty ;
                               rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                               rdfs:range OpenPVSignal:Patient ;
                               rdfs:comment "Identifies the patient that the respective spontaneous report refers to." ;
                               rdfs:label "refers to patient"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_pharmacovigilance_signal_report
OpenPVSignal:refers_to_pharmacovigilance_signal_report rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf mp:argues ;
                                                       rdfs:domain OpenPVSignal:Response_to_Pharmacovigilance_Signal_Report ;
                                                       rdfs:range OpenPVSignal:Pharmacovigilance_Signal_Report ;
                                                       rdfs:comment "Relates a pharmacovigilance signal response to the pharmacovigilance signal report." ;
                                                       rdfs:label "refers to pharmacovigilance signal report" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_primary_suspect_drug
OpenPVSignal:refers_to_primary_suspect_drug rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf OpenPVSignal:refers_to_drug ;
                                            rdf:type owl:FunctionalProperty ;
                                            rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                                            rdfs:comment "Explicitly states that the respective drug is reported as the primary suspect drug in the specific spontaneous report." ;
                                            rdfs:label "refers to primary suspect drug"@en .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_reported_drug_usage
OpenPVSignal:refers_to_reported_drug_usage rdf:type owl:ObjectProperty ,
                                                    owl:FunctionalProperty ;
                                           rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ,
                                                       OpenPVSignal:Reports_group ;
                                           rdfs:range OpenPVSignal:Drug_Usage ;
                                           rdfs:comment "Relates a spontaneous report with the respective drug usages." ;
                                           rdfs:label "refers to reported drug usage" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_secondary_suspect_drug
OpenPVSignal:refers_to_secondary_suspect_drug rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf OpenPVSignal:refers_to_drug ;
                                              rdfs:domain OpenPVSignal:Individual_Case_Safety_Report ;
                                              rdfs:comment "Explicitly states that the respective drug is reported as a secondary suspect drug in the specific spontaneous report." ;
                                              rdfs:label "refers to secondary suspect drug" .


###  http://purl.org/OpenPVSignal/OpenPVSignal.owl#refers_to_signal
OpenPVSignal:refers_to_signal rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf mp:argues ;
